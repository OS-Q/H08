{
  "name": "H8",
  "title": "H8",
  "description": "ESP8266/8285开发平台",
  "url": "https://espressif.com/",
  "homepage": "http://OS-Q.com/H8",
  "license": " ",
  "engines": {
    "platformio": "<5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OS-Q/H8.git"
  },
  "version": "1.0.0",
  "packageRepositories": [
    "https://dl.bintray.com/platformio/dl-packages/manifest.json",
    "http://dl.platformio.org/packages/manifest.json"
  ],
  "frameworks": {
    "arduino": {
      "package": "framework-N11",
      "script": "builder/frameworks/arduino.py"
    },
    "simba": {
      "package": "framework-N16",
      "script": "builder/frameworks/simba.py"
    },
    "rtos8266": {
      "package": "framework-N12",
      "script": "builder/frameworks/rtos8266.py"
    },
    "esp8266": {
      "package": "framework-N13",
      "script": "builder/frameworks/esp8266.py"
    }
  },
  "packages": {
    "framework-N11": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/N11.git"
    },
    "framework-N12": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/N12.git"
    },
    "framework-N13": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/N13.git"
    },
    "framework-N16": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/N16.git"
    },
    "toolchain-xtensa": {
      "type": "toolchain",
      "version": "<3"
    },
    "tool-esptool": {
      "type": "uploader",
      "version": "<2"
    },
    "tool-esptoolpy": {
      "type": "uploader",
      "version": "~1.20600.0"
    },
    "tool-mkspiffs": {
      "type": "uploader",
      "optional": true,
      "version": "~1.200.0"
    }
  }
}
