{
  "name": "P15",
  "title": "Platform P15",
  "description": "Espressif wifi SoC.",
  "homepage": "https://espressif.com/",
  "license": "Apache-2.0",
  "keywords": [
    "dev-platform",
    "OS-Q",
    "Wi-Fi",
    "Xtensa",
    "106Micro"
  ],
  "engines": {
    "platformio": "^5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OS-Q/P15.git"
  },
  "version": "2.6.4",
  "frameworks": {
    "arduino": {
      "package": "A15",
      "script": "builder/frameworks/arduino.py"
    },
    "simba": {
      "package": "simba",
      "script": "builder/frameworks/simba.py"
    },
    "rtos": {
      "package": "F15",
      "script": "builder/frameworks/rtos.py",
      "title": "ESP8266 RTOS SDK"
    },
    "native": {
      "package": "N15",
      "script": "builder/frameworks/native.py",
      "description": "The non-OS SDK provides a set of application programming interfaces (APIs) for core ESP8266 functionalities",
      "homepage": "https://github.com/espressif/ESP8266_NONOS_SDK",
      "title": "ESP8266 SDK"
    }
  },
  "packages": {
    "toolchain-xtensa": {
      "type": "toolchain",
      "owner": "platformio",
      "version": "~2.40802.0",
      "optionalVersions": ["~1.40802.0"]
    },
    "A15": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/A15.git"
    },
    "F15": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/F15.git"
    },
    "N15": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/N15.git"
    },
    "simba": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/simba.git"
    },
    "tool-esptool": {
      "type": "uploader",
      "owner": "platformio",
      "version": "<2"
    },
    "tool-esptoolpy": {
      "type": "uploader",
      "owner": "platformio",
      "version": "~1.30000.0"
    },
    "tool-mkspiffs": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "~1.200.0"
    },
    "tool-mklittlefs": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "~1.203.0"
    }
  }
}
